<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cs3773project.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
   
       <Label fx:id="welcomeText" />
      <Button mnemonicParsing="false" text="Orders" />
      <Button fx:id="NewUser" mnemonicParsing="false" onAction="#action1" text="New User" />
       <Button fx:id="item" onMouseClicked="#goItems" text="Items" />
   </children>
</VBox>
