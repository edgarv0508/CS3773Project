   <children>
      <TextField fx:id="tfCode" layoutX="350.0" layoutY="73.0" promptText="Code" />
      <TextField fx:id="tfPercent" layoutX="350.0" layoutY="188.0" promptText="Percent" />
      <Button fx:id="create" layoutX="60.0" layoutY="139.0" mnemonicParsing="false" onMouseClicked="#create" text="Create Discount Code" />
      <Button fx:id="home" layoutX="106.0" layoutY="61.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
